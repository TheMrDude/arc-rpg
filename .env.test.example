# TEST ENVIRONMENT VARIABLES
# Copy this file to .env.test and fill in your test values
# NEVER commit .env.test with real credentials!

# API Base URL (local development server)
TEST_API_BASE=http://localhost:3000

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Test User Credentials
# Create test users in your Supabase auth
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=test_password_123
TEST_USER_TOKEN=your_test_user_jwt_token
TEST_USER_ID=your_test_user_uuid

# Premium Test User (for premium-specific tests)
TEST_PREMIUM_USER_EMAIL=premium@example.com
TEST_PREMIUM_USER_TOKEN=your_premium_user_jwt_token
TEST_PREMIUM_USER_ID=your_premium_user_uuid

# Admin User (for admin endpoint tests)
TEST_ADMIN_EMAIL=admin@example.com
TEST_ADMIN_TOKEN=your_admin_user_jwt_token
ADMIN_EMAILS=admin@example.com,test-admin@example.com

# Test Quest ID (create a quest for testing completion)
TEST_QUEST_ID=your_test_quest_uuid

# Other Test User (for cross-user access tests)
TEST_OTHER_USER_TOKEN=another_user_jwt_token
TEST_OTHER_USER_ID=another_user_uuid

# Stripe Test Keys (use test mode only!)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_xxx
STRIPE_SECRET_KEY=sk_test_xxx
STRIPE_WEBHOOK_SECRET=whsec_test_xxx

# Anthropic API (use test account with rate limits)
ANTHROPIC_API_KEY=sk-ant-xxx

# Test Configuration
NODE_ENV=test
